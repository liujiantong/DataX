{
    "job": {
        "setting": {
            "speed": {
                "byte":10485760
            },
            "errorLimit": {
                "record": 0,
                "percentage": 0.02
            }
        },
        "content": [
            {
                "reader": {
                    "name": "oraclereader",
                    "parameter": {
                        "username": "liutao",
                        "password": "liutao",
                        "column": [
                            "id", "nickname", "email", "mobile", "memberid", "cardid", "mobilestate", "emailstate"
                        ],
                        "where": "regtime > to_date('2016-01-01', 'YYYY-MM-DD')",
                        "connection": [
                            {
                                "table": [
                                    "mis_login_user"
                                ],
                                "jdbcUrl": [
                                    "jdbc:oracle:thin:@192.168.99.214:1521:ikangrac"
                                ]
                            }
                        ]
                    }
                },
                "writer": {
                    "name": "titandbwriter",
                    "parameter": {
                        "titandbConf": "conf/titan-berkeleyje.properties",
                        "column": [
                            {
                                "name": "userId",
                                "type": "long"
                            },
                            {
                                "name": "nickname",
                                "type": "string"
                            },
                            {
                                "name": "email",
                                "type": "string"
                            },
                            {
                                "name": "mobile",
                                "type": "string"
                            },
                            {
                                "name": "memberId",
                                "type": "long"
                            },
                            {
                                "name": "cardno",
                                "type": "string"
                            },
                            {
                                "name": "mobileState",
                                "type": "integer"
                            },
                            {
                                "name": "emailState",
                                "type": "integer"
                            }
                        ],
                        "graph": {
                            "vertices": [
                                {
                                    "label": "user",
                                    "properties": [
                                        {
                                            "name": "userId",
                                            "column": "userId",
                                            "index": "unique"
                                        },
                                        {
                                            "name": "nickname",
                                            "column": "nickname",
                                            "index": "composite",
                                            "required": true
                                        }
                                    ],
                                    "edges": [
                                        {
                                            "label": "hasMobile",
                                            "vertex": "mobile",
                                            "multiplicity": "one2one",
                                            "weight": 1.0
                                        },
                                        {
                                            "label": "hasEmail",
                                            "vertex": "email",
                                            "multiplicity": "one2one",
                                            "weight": 1.0
                                        },
                                        {
                                            "label": "hasMemberId",
                                            "vertex": "memberId",
                                            "multiplicity": "one2one",
                                            "weight": 1.0
                                        },
                                        {
                                            "label": "hasCardno",
                                            "vertex": "cardno",
                                            "multiplicity": "one2one",
                                            "weight": 1.0
                                        }
                                    ]
                                },
                                {
                                    "label": "email",
                                    "properties": [
                                        {
                                            "name": "email",
                                            "column": "email",
                                            "index": "unique",
                                            "required": true,
                                            "pattern": "\\w+@\\w+\\.\\w{2,}"
                                        },
                                        {
                                            "name": "state",
                                            "column": "emailState"
                                        }
                                    ]
                                },
                                {
                                    "label": "mobile",
                                    "properties": [
                                        {
                                            "name": "mobile",
                                            "column": "mobile",
                                            "index": "unique",
                                            "required": true,
                                            "pattern": "1\\d{10}"
                                        },
                                        {
                                            "name": "state",
                                            "column": "mobileState"
                                        }
                                    ]
                                },
                                {
                                    "label": "memberId",
                                    "properties": [
                                        {
                                            "name": "memberId",
                                            "column": "memberId",
                                            "index": "unique",
                                            "required": true
                                        }
                                    ]
                                },
                                {
                                    "label": "cardno",
                                    "properties": [
                                        {
                                            "name": "cardno",
                                            "column": "cardno",
                                            "index": "unique",
                                            "required": true
                                        }
                                    ]
                                }
                            ]
                        }
                    }
                }
            }
        ]
    }
}
